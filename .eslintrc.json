{
    "extends": "airbnb-base",
    "rules": {
        "import/newline-after-import": "off",
        "linebreak-style": "off",
        "func-names": "off",
        "semi": "off",
        "no-console": "off",
        "eol-last": "off",
        "prefer-const": "off",
        "quotes": "off",
        "prefer-destructuring": "off",
        "curly": "off",
        "yoda": "off",
        "comma-dangle": "off",
        "consistent-return": "off",
        "no-trailing-spaces": "off",
        "no-restricted-syntax": "off",
        "guard-for-in": "off",
        "prefer-template": "off",
        "quote-props": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "off",
        "import/prefer-default-export": "off",
        "import/no-cycle": "off"
    },
    "env": {
        "mocha": true,
        "browser": true
    }
}