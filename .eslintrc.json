{
    "extends": "airbnb-base",
    "rules": {
        "import/newline-after-import": "off",
        "linebreak-style": "off",
        "func-names": "off",
        "semi": "off",
        "no-console": "off",
        "eol-last": "off",
        "prefer-const": "off",
        "quotes": "off",
        "prefer-destructuring": "off",
        "curly": "off",
        "yoda": "off",
        "comma-dangle": "off",
        "consistent-return": "off",
        "no-trailing-spaces": "off"
    },
    "env": {
        "mocha": true
    }
}